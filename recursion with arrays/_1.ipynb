   "source": [
    "### write a program to check if array is sorted ?"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "True\n"
     ]
    }
   ],
   "source": [
    "def check_sort(l1):\n",
    "\n",
    "    if (len(l1)==0 or len(l1)==1):\n",
    "        return True\n",
    "    \n",
    "    ans= check_sort(l1[1:])\n",
    "    if (l1[0]>l1[1]):\n",
    "        return False\n",
    "    else:\n",
    "        return ans\n",
    "    \n",
    "\n",
    "print(check_sort([1,2,3,4,5]))\n",
    "\n"
   ]
  },
